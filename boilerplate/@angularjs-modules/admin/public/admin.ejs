<!DOCTYPE html>
<html lang="<%= lang %>">

<head>
    <meta charset="utf-8">
    <title>ADMIN <%= HOSTNAME %> <%= lang %></title>

    <!-- static -->

    <link rel="stylesheet" href="/css/bootstrap.min.css">
    <link rel="stylesheet" href="/font-awesome.min.css">
    <link rel="stylesheet" href="/animate.min.css">

    <link rel="stylesheet" href="/common.css">
    <link rel="stylesheet" href="/smartforms.css">

    <link rel="stylesheet" href="/admin.css">
    <link rel="stylesheet" href="/jsoneditor.min.css">

    <!-- bp -->
    <%- styles %>

        <!-- static -->

        <script src="/jquery.min.js" type="text/javascript"></script>
        <script src="/angular.min.js" type="text/javascript"></script>
        <script src="/socket.io/socket.io.js" type="text/javascript"></script>
        <script src="/socket.io-stream.js"></script>

        <script src="/ng-file-upload-shim.min.js"></script>
        <script src="/ng-file-upload.min.js"></script>

        <script src="/js/bootstrap.min.js"></script>
        <script src="/ng-jsoneditor.min.js" type="text/javascript"></script>
        <script src="/jsoneditor.min.js" type="text/javascript"></script>

        <script>
            var app = angular.module('app', ['ngFileUpload', 'ng.jsoneditor']);
        </script>

        <script src="/smartforms.js" type="text/javascript"></script>

        <!-- bp -->
        <%- scripts %>


</head>

<body ng-app="app" ng-controller="mainController">

    <div class="alertServiceAlerts">
        <div class="alertServiceAlert alert-{{alert.type}} animated {{alert.anim}}" ng-repeat="alert in alerts track by $index" type="alert.type">{{ alert.msg }}</div>
    </div>

    <div id="navigation" ng-cloak ng-include="'/navigation.html'" class="navigation">
    </div>

    <div id="page" ng-cloak ng-include="pageuri" class="animated fadeIn {{page}}" ng-controller="editorController" contenteditable="true">
    </div>
</body>

</html>
